{
  "name": "redovisa",
  "version": "1.0.0",
  "description": "Express and React-app",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "concurrently \"npm run mocha\" \"npm run stylelint\" \"npm run eslint\"",
    "mocha": "nyc --reporter=html --reporter=text mocha --reporter spec \"test/**/*.spec.js\"",
    "stylelint": "node_modules/.bin/stylelint **/*.css",
    "eslint": "node_modules/.bin/eslint --ignore-path .eslintignore .",
    "eslint-fix": "node_modules/.bin/eslint --ignore-path .eslintignore --fix .",
    "coverage-scrut": "nyc --reporter=clover mocha --reporter spec \"test/**/*.spec.js\"",
    "start-docker": "docker-compose up -d express",
    "stop-docker": "docker-compose down",
    "test-docker": "docker-compose up test",
    "test-docker1": "docker-compose up test2",
    "test-docker2": "docker-compose up test3"
  },
  "author": "Nicklas Envall",
  "license": "MIT",
  "dependencies": {
    "ajv": "^5.5.2",
    "body-parser": "~1.18.2",
    "concurrently": "^3.5.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "express-session": "^1.15.6",
    "mongo-connecter": "^1.0.4",
    "mongodb": "^2.2.33",
    "morgan": "~1.9.0",
    "postcss-sass": "^0.2.0",
    "serve-favicon": "~2.4.5",
    "socket-mansion": "^1.0.3",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-css-cli": "^4.1.10",
    "coveralls": "^3.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-react": "^7.5.1",
    "js-yaml": "^3.10.0",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "stylelint": "^8.3.1",
    "supertest": "^3.0.0",
    "uglify-js": "^3.1.6"
  }
}
